{
  "name": "dappy",
  "repository": "https://github.com/fabcotech/dappy",
  "productName": "dappy",
  "description": "Dappy is a no-DNS and no-CAs web browser, it works hand in hand with a blockchain platform. The dappy system is a decentralized alternative to the regular web, it allows for new kind of web applications being built and used by human beings and corporations.",
  "homepage": "https://github.com/fabcotech/dappy",
  "author": "FABCO <contact@fabco.tech>",
  "version": "0.5.7",
  "main": "dist/main/main.js",
  "scripts": {
    "check": "tsc --pretty --jsx react --noEmit",
    "start:renderer": "npx parcel serve src/index.html -p 3033 --target browser",
    "start:main": "cross-env NODE_ENV=development electron . --unhandled-rejections=strict --trace-warnings",
    "clean:dist": "rm -rf dist",
    "build:main:dev": " mkdir -p dist/renderer/ && cp -r dapp-libs/* dist/renderer/ && npx rollup -c rollup.main.config.js",
    "build:main:dev:watch": "npm run build:main:dev -- -w",
    "build:renderer:prod": "cross-env NODE_ENV=production npx parcel build src/index.html --public-url . --out-dir dist/renderer && cp -r dapp-libs/* dist/renderer/",
    "build:main:prod": "cross-env PRODUCTION=true NODE_ENV=production npx rollup -c rollup.main.config.js",
    "build:all:prod": "npm run clean:dist && npm run build:renderer:prod && npm run build:main:prod",
    "build:dappyrchain": "cross-env NODE_ENV=production npx rollup -c rollup.dappy-rchain.config.js",
    "build:dappyethereum": "cross-env NODE_ENV=production npx rollup -c rollup.dappy-ethereum.config.js",
    "package:linux:deb": "./scripts/outdated.sh electron && electron-builder -c electron-builder.yml --x64 --linux deb -p onTagOrDraft",
    "package:linux:rpm": "./scripts/outdated.sh electron && electron-builder -c electron-builder.yml --x64 --linux rpm -p onTagOrDraft",
    "package:linux:app-image": "./scripts/outdated.sh electron && electron-builder -c electron-builder.yml --x64 --linux AppImage -p onTagOrDraft",
    "package:windows:64": "electron-builder -c electron-builder.yml --x64 --win nsis -p onTagOrDraft",
    "package:windows:32": "electron-builder -c electron-builder.yml --ia32 --win nsis -p onTagOrDraft",
    "package:mac": "./scripts/outdated.sh electron && electron-builder -c electron-builder.yml --mac -p onTagOrDraft",
    "security-audit": "./scripts/outdated.sh electron && npm audit --production --audit-level=high",
    "test": "jest",
    "test:integration:gamma": "cross-env PARTITION=test DAPPY_NETWORK=gamma playwright test",
    "test:integration:d": "cross-env PARTITION=test DAPPY_NETWORK=d playwright test",
    "lint": "eslint src --ext .ts",
    "test:types": "npx tsc --project ./tsconfig.json --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ethereumjs/common": "^2.6.0",
    "@ethereumjs/tx": "^3.4.0",
    "@fabcotech/bees": "2.0.5",
    "@fabcotech/dappy-lookup": "^2.3.14",
    "@fabcotech/rchain-token": "17.0.6",
    "@fabcotech/rchain-toolkit": "3.0.0",
    "@sentry/browser": "6.7.1",
    "ajv": "^6.12.6",
    "blakejs": "^1.1.0",
    "bulma": "0.9.3",
    "bulma-extensions": "^6.2.7",
    "dappy-reserved-domains": "github:fabcotech/dappy-reserved-domains",
    "electron": "^21.0.0",
    "electron-updater": "^5.0.5",
    "elliptic": "^6.5.4",
    "formik": "2.2.9",
    "luxon": "^1.3.3",
    "pem": "^1.14.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.5",
    "redux": "4.1.1",
    "redux-saga": "^1.1.3",
    "reselect": "^3.0.1",
    "set-cookie-parser": "^2.4.6",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "uuid": "^3.4.0",
    "web3-utils": "^1.6.1",
    "xstream": "^11.7.0",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@playwright/test": "^1.27.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@swc/core": "^1.3.10",
    "@swc/jest": "^0.2.23",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.3.0",
    "@types/chrome": "0.0.71",
    "@types/elliptic": "^6.4.13",
    "@types/jest": "^27.0.2",
    "@types/luxon": "^1.4.1",
    "@types/pem": "^1.9.6",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/set-cookie-parser": "^2.4.1",
    "@types/uuid": "^3.4.7",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "electron-builder": "^23.3.3",
    "electron-notarize": "^1.2.1",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "parcel-bundler": "1.12.5",
    "parcel-plugin-package-version": "^1.0.2",
    "playwright": "^1.27.0",
    "prettier": "2.5.1",
    "rollup": "1.26.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript": "^1.0.1",
    "sass": "1.26.5",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^27.0.5",
    "typescript": "^4.6.2"
  }
}