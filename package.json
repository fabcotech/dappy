{
  "name": "dappy",
  "productName": "dappy",
  "description": "Dappy is a no-DNS and no-CAs web browser, it works hand in hand with a blockchain platform. The dappy system is a decentralized alternative to the regular web, it allows for new kind of web applications being built and used by human beings and corporations.",
  "homepage": "https://github.com/fabcotech/dappy",
  "author": "FABCO <contact@fabco.tech>",
  "version": "0.5.5",
  "main": "main.js",
  "scripts": {
    "check": "tsc --pretty --jsx react --noEmit",
    "start:parcel": "npx parcel serve src/index.html -p 3033 --target browser",
    "start:electron": "export NODE_ENV=development && electron --unhandled-rejections=strict --trace-warnings .",
    "build": "export NODE_ENV=production && rm -rf dist/ && npx parcel build src/index.html --public-url . && cp -r dapp-libs/* dist/ && cp dapp.html dist/",
    "build:dappyrchain": "NODE_ENV=production npx rollup -c rollup.dappy-rchain.config.js",
    "build:dappyethereum": "NODE_ENV=production npx rollup -c rollup.dappy-ethereum.config.js",
    "build:main": "NODE_ENV=production npx rollup -c rollup.main.config.js",
    "build:main:dev": "npx rollup -c rollup.main.config.js",
    "build:main:dev:watch": "npx rollup -c rollup.main.config.js -w",
    "electron-builder:dist:linux:deb": "./outdated.sh electron && electron-builder -c build.yml --x64 --publish never --linux deb",
    "electron-builder:dist:linux:rpm": "./outdated.sh electron && electron-builder -c build.yml --x64 --publish never --linux rpm",
    "electron-builder:dist:windows:64": "electron-builder -c build.yml --x64 --publish never --win nsis",
    "electron-builder:dist:windows:32": "electron-builder -c build.yml --ia32 --publish never --win nsis",
    "electron-builder:dist:mac": "./outdated.sh electron && electron-builder -c build.yml --publish never --mac",
    "security-audit": "./outdated.sh electron && npm audit --production --audit-level=high",
    "test": "jest",
    "prepare": "husky install"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "parser": "typescript",
    "trailingComma": "es5"
  },
  "dependencies": {
    "@ethereumjs/common": "^2.6.0",
    "@ethereumjs/tx": "^3.4.0",
    "@fabcotech/bees": "2.0.5",
    "@fabcotech/dappy-lookup": "2.2.2",
    "@sentry/browser": "6.7.1",
    "ajv": "^6.12.6",
    "blakejs": "^1.1.0",
    "bulma": "0.9.3",
    "bulma-extensions": "^6.2.7",
    "dappy-reserved-domains": "github:fabcotech/dappy-reserved-domains",
    "dns-packet": "^5.3.1",
    "elliptic": "^6.5.3",
    "formik": "2.2.9",
    "luxon": "^1.3.3",
    "pem": "^1.14.6",
    "rchain-token": "fabcotech/rchain-token#16.0.0",
    "rchain-toolkit": "0.4.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.5",
    "redux": "4.1.1",
    "redux-saga": "^1.1.3",
    "reselect": "^3.0.1",
    "set-cookie-parser": "^2.4.6",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "uuid": "^3.4.0",
    "web3-utils": "^1.6.1",
    "xstream": "^11.7.0",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^3.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.3.0",
    "@types/chrome": "0.0.71",
    "@types/elliptic": "^6.4.9",
    "@types/jest": "^27.0.2",
    "@types/luxon": "^1.4.1",
    "@types/pem": "^1.9.6",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/set-cookie-parser": "^2.4.1",
    "@types/uuid": "^3.4.7",
    "@types/yup": "^0.29.13",
    "cssnano": "^4.1.10",
    "electron": "^18.1.0",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "parcel-bundler": "1.12.5",
    "prettier": "2.5.1",
    "rollup": "1.26.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript": "^1.0.1",
    "sass": "1.26.5",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^27.0.5",
    "typescript": "^4.6.2"
  }
}
